{
  "name": "nextjs-fullstack-boilerplate",
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "next build",
    "dev": "prisma migrate dev && next dev -p 80",
    "format:prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,cjs,mjs,cts,mts,json}\"",
    "format:prisma": "prisma format",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:secret": "secretlint \"**/*\"",
    "postinstall": "prisma generate",
    "start": "node server.js",
    "test": "jest"
  },
  "dependencies": {
    "@nextui-org/react": "2.4.8",
    "@prisma/client": "5.18.0",
    "framer-motion": "11.11.8",
    "next": "14.2.15",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.13.0",
    "@secretlint/secretlint-rule-preset-recommend": "8.5.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "16.0.1",
    "@types/jest": "29.5.13",
    "@types/node": "20.16.11",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "9.1.0",
    "globals": "15.11.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lefthook": "1.7.18",
    "npm-run-all2": "6.2.6",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prisma": "5.18.0",
    "secretlint": "8.5.0",
    "tailwindcss": "3.4.13",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3",
    "typescript-eslint": "8.9.0"
  }
}
