{
  "name": "nextjs-fullstack-boilerplate",
  "packageManager": "npm@10.8.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "run-s migrate build:*",
    "build:next": "next build",
    "dev": "run-p dev:* migrate",
