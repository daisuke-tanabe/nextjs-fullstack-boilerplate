{
  "name": "nextjs-fullstack-boilerplate",
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "next build",
    "dev": "dotenv -e .env.local -- prisma migrate dev && next dev --turbopack",
    "format": "run-p format:*",
    "format:prettier": "prettier --write \"**/*.{js,ts,jsx,tsx,cjs,mjs,cts,mts,json}\"",
    "format:prisma": "prisma format",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:secret": "secretlint \"**/*\"",
    "lint-fix": "run-p lint-fix:*",
    "lint-fix:eslint": "eslint . --fix",
    "postinstall": "dotenv -e .env.development.local -- prisma generate",
    "start": "node server.js",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@fontsource/roboto": "5.2.5",
    "@mui/icons-material": "6.4.5",
    "@mui/material": "6.4.5",
    "@mui/material-nextjs": "6.4.3",
    "@mui/system": "6.4.3",
    "@prisma/client": "6.3.1",
    "@supabase/ssr": "0.6.1",
    "@supabase/supabase-js": "2.49.4",
    "next": "15.3.2",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.27.0",
    "@iconify/react": "5.2.1",
    "@secretlint/secretlint-rule-preset-recommend": "9.2.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.14.1",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "autoprefixer": "10.4.21",
    "dotenv-cli": "8.0.0",
    "eslint": "9.27.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "globals": "15.15.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lefthook": "1.11.13",
    "npm-run-all2": "7.0.2",
    "prettier": "3.5.3",
    "prisma": "6.3.1",
    "secretlint": "9.2.0",
    "supabase": "2.20.12",
    "ts-jest": "29.3.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.21.0"
  }
}
